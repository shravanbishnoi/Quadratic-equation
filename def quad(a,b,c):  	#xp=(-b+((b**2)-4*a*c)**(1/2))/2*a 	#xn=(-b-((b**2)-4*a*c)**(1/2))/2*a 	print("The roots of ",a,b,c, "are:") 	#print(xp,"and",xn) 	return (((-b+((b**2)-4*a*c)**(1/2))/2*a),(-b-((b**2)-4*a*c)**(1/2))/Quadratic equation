#creating a function which gives roots of quadratic equation.
def quad(a,b,c):

	#xp=(-b+((b**2)-4*a*c)**(1/2))/2*a
	#xn=(-b-((b**2)-4*a*c)**(1/2))/2*a
	print("The roots of ",a,b,c, "are:")
	#print(xp,"and",xn)
	return (((-b+((b**2)-4*a*c)**(1/2))/2*a),(-b-((b**2)-4*a*c)**(1/2))/2*a)

roots=quad(1,5,6)
print(roots)
